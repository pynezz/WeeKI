# WeeKI Configuration
# Copy this file to .env and modify as needed

# Server Configuration
WEEKI_HOST=0.0.0.0
WEEKI_PORT=8000
WEEKI_DEBUG=false

# API Configuration
WEEKI_API_TITLE=WeeKI API
WEEKI_API_VERSION=0.1.0
WEEKI_API_DESCRIPTION=AI Agent Orchestration System API

# Security
WEEKI_SECRET_KEY=change-me-in-production-use-a-long-random-string

# Database Configuration
WEEKI_DATABASE_URL=sqlite:///./weeki.db

# Agent Configuration
WEEKI_MAX_AGENTS=10
WEEKI_AGENT_TIMEOUT=300

# External API Keys (optional)
# WEEKI_OPENAI_API_KEY=your-openai-api-key
# WEEKI_ANTHROPIC_API_KEY=your-anthropic-api-key

# Logging
WEEKI_LOG_LEVEL=INFO
WEEKI_LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s